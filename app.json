{
  "expo": {
    "name": "yachay-kimiku",
    "slug": "yachay-kimiku",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sairecnj.yachaykimiku"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "63b1bff1-ce65-414f-9e3e-31c608b21ede"
      }
    },
    "assetBundlePatterns": [
      "assets/**/*",
      "assets/fonts/*"
    ]
  }
}
